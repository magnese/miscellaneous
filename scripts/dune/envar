GMSH_DIR=$DUNE_DIR/gmsh-trunk; export GMSH_DIR

PATH=$GMSH_DIR/bin:$PATH; export PATH
LD_LIBRARY_PATH=$GMSH_DIR/lib:$SUITESPARSE_DIR/lib:$LD_LIBRARY_PATH;export LD_LIBRARY_PATH
DUNE_CONTROL_PATH=$DUNE_DIR:$MYDUNEMODULES_DIR; export DUNE_CONTROL_PATH
PKG_CONFIG_PATH=$DUNE_DIR:$MYDUNEMODULES_DIR; export PKG_CONFIG_PATH
